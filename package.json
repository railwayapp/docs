    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "prisma": "4.10.1",
    "remark-autolink-headings": "^7.0.1",
    "remark-slug": "^7.0.1",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=18.17.0"
