    "@tailwindcss/typography": "^0.5.9",
    "classnames": "^2.3.2",
    "contentlayer": "^0.3.4",
    "cookies-next": "^4.2.1",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.10.4",
    "fathom-client": "^3.1.0",
    "graphql": "^16.8.1",
    "interweave": "^13.1.0",
    "meilisearch": "^0.32.3",
    "nanostores": "^0.7.4",
    "next": "^14.2.5",
    "next-contentlayer": "^0.3.4",
    "next-seo": "^5.15.0",
    "next-themes": "^0.2.1",
